{
  "name": "webpack-unbundle",
  "version": "0.1.0",
  "description": "Unbundle a bundled webpack project into separate files.",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "files": [
    "dist/"
  ],
  "keywords": [
    "debundle",
    "unbundle",
    "webpack",
    "deobsfuscate",
    "deobsfuscator"
  ],
  "scripts": {
    "dev": "esbuild src/index.ts --bundle --platform=node --packages=external --outdir=dist --watch",
    "prepack": "tsc"
  },
  "author": "awphi (adamw.ph)",
  "license": "GPL-3.0-only",
  "dependencies": {
    "commander": "^12.0.0",
    "recast": "^0.23.6"
  },
  "devDependencies": {
    "@types/node": "20.11.0",
    "esbuild": "^0.20.2",
    "typescript": "^5.4.2"
  }
}
