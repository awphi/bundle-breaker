{
  "name": "bundle-breaker",
  "version": "0.1.0",
  "description": "Utilities to reverse-engineer built webpack bundles.",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "packageManager": "pnpm@8.15.4",
  "files": [
    "dist/"
  ],
  "keywords": [
    "debundle",
    "unbundle",
    "webpack",
    "deobsfuscate",
    "deobsfuscator"
  ],
  "scripts": {
    "dev": "pnpm build --watch",
    "prepack": "pnpm build",
    "build": "tsc",
    "examples:build-all": "node example-builder.js build-all",
    "examples:build": "node example-builder.js build"
  },
  "author": "awphi (adamw.ph)",
  "license": "GPL-3.0-only",
  "dependencies": {
    "acorn": "^8.11.3",
    "commander": "^12.0.0",
    "recast": "^0.23.6",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/node": "20.11.0",
    "typescript": "^5.4.2"
  }
}
